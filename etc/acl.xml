<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Websoft_Daftra::Log" title="Log" sortOrder="10">
					<resource id="Websoft_Daftra::Log_save" title="Save Log" sortOrder="10"/>
					<resource id="Websoft_Daftra::Log_delete" title="Delete Log" sortOrder="20"/>
					<resource id="Websoft_Daftra::Log_update" title="Update Log" sortOrder="30"/>
					<resource id="Websoft_Daftra::Log_view" title="View Log" sortOrder="40"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Websoft_Daftra::config_websoft_daftra" title="general"/>
						</resource>
					</resource>
				</resource>
				<resource id="Websoft_Daftra::GuestClient" title="GuestClient" sortOrder="10">
					<resource id="Websoft_Daftra::GuestClient_save" title="Save GuestClient" sortOrder="10"/>
					<resource id="Websoft_Daftra::GuestClient_delete" title="Delete GuestClient" sortOrder="20"/>
					<resource id="Websoft_Daftra::GuestClient_update" title="Update GuestClient" sortOrder="30"/>
					<resource id="Websoft_Daftra::GuestClient_view" title="View GuestClient" sortOrder="40"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
